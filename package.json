{
    "name": "vscode-md2hatena",
    "displayName": "vscode-md2hatena",
    "description": "markdown to hatena language",
    "publisher": "cohalz",
    "repository": {
        "type": "git",
        "url": "https://github.com/cohalz/vscode-md2hatena"
    },
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.md2hatena"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.md2hatena",
                "title": "md2hatena: Convert Markdown To Hatena"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
